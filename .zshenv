export EDITOR=vim
export PAGER=less
export LV="-Ou8 -c -l"
export BLOCKSIZE=K

export BROWSER=opera
export PSPDEV="/opt/pspsdk"
export CCACHE_PATH="/usr/bin"
export CCACHE_DIR="/tmp/ccache"
export CCACHE_SIZE=2G

export DE=xfce
export GTK2_RC_FILES="$HOME/.gtkrc-2.0"
export GNOME_DESKTOP_SESSION_ID=1

export LIBVA_DRIVER_NAME=vdpau
export VDPAU_DRIVER=r600
export R600_STREAMOUT=1
export R600_HYPERZ=1
export R600_TILING=1
export R600_SURF=1
export R600_GLSL130=1

export GDK_USE_XFT=1
export QT_XFT=true

export VTE_CJK_WIDTH=1

export CHROMIUM_USER_FLAGS="--disk-cache-dir=/tmp/cache --disk-cache-size=50000000 --ignore-gpu-blacklist --enable-webgl --ppapi-flash-path=/usr/lib/PepperFlash/libpepflashplayer.so --ppapi-flash-version=11.9.900.170"
