export EDITOR=vim
export PAGER=less
export LV="-Ou8 -c -l"
export BLOCKSIZE=K

export BROWSER=opera
export PSPDEV="/opt/pspsdk"
export CCACHE_PATH="/usr/bin"
export CCACHE_DIR="/tmp/ccache"
export CCACHE_SIZE=2G

export LIBVA_DRIVER_NAME=gallium
export VDPAU_DRIVER=radeonsi
export R600_STREAMOUT=1
export R600_HYPERZ=1
export R600_TILING=1
export R600_SURF=1
export R600_GLSL130=1

export GDK_USE_XFT=1
export QT_XFT=true

export VTE_CJK_WIDTH=1

export CHROMIUM_USER_FLAGS="--lang=ja-JP --disk-cache-dir=/tmp/cache --disk-cache-size=50000000 --ignore-gpu-blacklist --enable-webgl --ppapi-flash-path=/usr/lib/PepperFlash/libpepflashplayer.so --ppapi-flash-version=16.0.0.296"

export XIM='ibus'
export GTK_IM_MODULE='ibus'
export QT_IM_MODULE='ibus'
export XIM_PROGRAM='/usr/bin/ibus-daemon'
export XIM_ARGS='--xim'
export XMODIFIERS=@im='ibus'

export OOO_FORCE_DESKTOP="kde4"

export AWS_HOME=$HOME/.aws
export EC2_URL=http://ec2.ap-northeast-1.amazonaws.com
export AWS_CREDENTIAL_FILE="$AWS_HOME/aws-crendetial-file"
export AWS_ACCESS_KEY= #Your Access Key
export AWS_SECRET_KEY= #Your Secret Key
